<div class="modal-body">
  <div class="row">
    <div class="col-sm-4">
      <h4>Info</h4>
      <dl>
        <dd>File Size:</dd>
        <dt>{{image.size}}</dt> 
      </dl>

      <dl>
        <dd>Percent Savings:</dd>
        <dt>{{image.size}}</dt> 
        <dd>TIme to Compress:</dd>
        <dt>{{image.compression_time.toFixed(4)}}s</dt> 
      </dl>
    </div>
  
    <div class="col-sm-8"> 
      <h4>Compression Settings</h4>
      <dl class="col-sm-4" ng-repeat="(worker, settings) in image.compression_settings">
        <dt>{{worker}}</dt>
        <dd>
          <span ng-repeat="(key, value) in settings">
            {{key}}: {{value}}<br>
          </span>
        </dd>
      </dl>
    </div>
  </div>

</div>
